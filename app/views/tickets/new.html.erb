<h1> Upload Ticket </h1>

<%=form_for @ticket do |f|%>
<%=f.hidden_field :user_id, :value => current_user.id %>
<%=f.label "artist name" %>
<%=f.text_field :artist_name%>
<%=f.label "event date" %>
<%=f.date_field :event_date %>
<%=f.collection_select :venue_id, @venues, :id, :name %>
<%=f.submit "Submit" %>
<% end %>
